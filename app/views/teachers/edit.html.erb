<h1> Edita el perfil del profesor </h1>


<%= form_for @teacher do |f| %>

  <form>
    <div class="form-group">
      <%= f.label :Profesor %>
      <%= f.text_field :name %>
    </div>
    <div class="form-group">
      <%= f.label :Responsabilidad %>
      <%= f.text_field :responsibility %>
    </div>
    <!-- <button type="submit" class="btn btn-primary"> AÃ±adir </button> -->
    <%= f.submit %>
  </form>

 <% end %>

